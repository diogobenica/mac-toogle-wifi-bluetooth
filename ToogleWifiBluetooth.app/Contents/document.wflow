					<key>COMMAND_STRING</key>
					<string>WIFI=$(/usr/sbin/networksetup -listallhardwareports | grep Wi-Fi -A2 | grep Device | sed 's/Device: //')

/usr/sbin/networksetup -getairportpower $WIFI | grep On

if [ $? -eq 0 ]; then
	/usr/sbin/networksetup -setairportpower $WIFI off
	/usr/local/bin/blueutil off
else
	/usr/sbin/networksetup -setairportpower $WIFI on
	/usr/local/bin/blueutil on
fi
</string>
